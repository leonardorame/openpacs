				join study s on s.idstudy=se.idstudy where s.modalityaetitle='''||rt.aesource||'''and i.idimage = '||new.idimage
				LOOP
					INSERT INTO routingtasks(
					idimage, idstudy, idserie, idrule, pathimage, 
