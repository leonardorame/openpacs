id_taskmanager_ integer;
idcleaningtask_ integer;
BEGIN

	delete from taskmanager 
	where idtaskmanager=idtaskmanager_;

	delete from taskscleaning 
	where idcleaningtask=idcleaningtask_;
			
RETURN id_taskmanager_; 
